<T3DataStructure>
  <meta>
    <langDisable>1</langDisable>
  </meta>
  <sheets>
    <sDEF>
      <ROOT>
        <TCEforms>
          <sheetTitle>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_tab.settings</sheetTitle>
          <cshFile>LLL:EXT:t3events/Resources/Private/Language/locallang_csh_flexform.xlf</cshFile>
        </TCEforms>
        <type>array</type>
        <el>

          <settings.showRadiusSearch>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.showRadiusSearch</label>
              <config>
                <type>check</type>
                <default>0</default>
              </config>
            </TCEforms>
          </settings.showRadiusSearch>

          <settings.showSearchField>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.showSearchField</label>
              <config>
                <type>check</type>
                <default>1</default>
              </config>
            </TCEforms>
          </settings.showSearchField>

          <view.pluginNamespace>
            <TCEforms>
              <label>Plugin Namespace</label>
              <config>
                <type>input</type>
                <eval>trim,nospace</eval>
              </config>
            </TCEforms>
          </view.pluginNamespace>

          <!-- caching -->
          <settings.cache.notCacheable>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.cache.makeNonCacheable</label>
              <config>
                <type>check</type>
              </config>
            </TCEforms>
          </settings.cache.notCacheable>
        </el>
      </ROOT>
    </sDEF>

    <!-- SHEET Constraints -->
    <constraints>
      <ROOT>
        <TCEforms>
          <sheetTitle>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.constraints</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
          <!-- statuses -->
          <settings.statuses>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.statuses</label>
              <config>
                <type>group</type>
                <internal_type>db</internal_type>
                <allowed>tx_t3events_domain_model_performancestatus</allowed>
                <size>3</size>
                <autoSizeMax>20</autoSizeMax>
                <minitems>0</minitems>
                <maxitems>20</maxitems>
                <wizards>
                  <suggest>
                    <type>suggest</type>
                  </suggest>
                </wizards>
              </config>
            </TCEforms>
          </settings.statuses>
          <settings.excludeSelectedStatuses>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.excludeSelectedStatuses</label>
              <config>
                <type>check</type>
              </config>
            </TCEforms>
          </settings.excludeSelectedStatuses>

          <settings.period>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.period</label>
              <onChange>reload</onChange>
              <config>
                <type>select</type>
                <renderType>selectSingle</renderType>
                <items type="array">
                  <numIndex index="0" type="array">
                    <numIndex index="0">LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.period.all</numIndex>
                    <numIndex index="1">all</numIndex>
                  </numIndex>
                  <numIndex index="1" type="array">
                    <numIndex index="0">LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.period.futureOnly</numIndex>
                    <numIndex index="1">futureOnly</numIndex>
                  </numIndex>
                  <numIndex index="2" type="array">
                    <numIndex index="0">LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.period.pastOnly</numIndex>
                    <numIndex index="1">pastOnly</numIndex>
                  </numIndex>
                  <numIndex index="3" type="array">
                    <numIndex index="0">LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.period.specific</numIndex>
                    <numIndex index="1">specific</numIndex>
                  </numIndex>
                </items>
              </config>
            </TCEforms>
          </settings.period>
          <legend>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.legendPeriodConstraints</label>
              <config>
                <type>user</type>
                <renderType>t3eventsLegendPeriodConstraints</renderType>              </config>
            </TCEforms>
          </legend>
          <settings.respectEndDate>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.respectEndDate</label>
              <displayCond>FIELD:settings.period:!=:all</displayCond>
              <onChange>reload</onChange>
              <config>
                <type>check</type>
              </config>
            </TCEforms>
          </settings.respectEndDate>

          <settings.periodType>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.constraintBy</label>
              <onChange>reload</onChange>
              <displayCond>FIELD:settings.period:=:specific</displayCond>
              <config>
                <type>radio</type>
                <items type="array">
                  <numIndex index="0" type="array">
                    <numIndex index="0">LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.period.specific.byDay</numIndex>
                    <numIndex index="1">byDay</numIndex>
                  </numIndex>
                  <numIndex index="1" type="array">
                    <numIndex index="0">LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.period.specific.byMonth</numIndex>
                    <numIndex index="1">byMonth</numIndex>
                  </numIndex>
                  <numIndex index="2" type="array">
                    <numIndex index="0">LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.period.specific.byYear</numIndex>
                    <numIndex index="1">byYear</numIndex>
                  </numIndex>
                  <numIndex index="3" type="array">
                    <numIndex index="0">LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.period.specific.byDate</numIndex>
                    <numIndex index="1">byDate</numIndex>
                  </numIndex>
                </items>
              </config>
            </TCEforms>
          </settings.periodType>
          <settings.periodStart>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.start</label>
              <displayCond>
                <AND>
                  <numIndex index="0">FIELD:settings.period:=:specific</numIndex>
                  <numIndex index="1">FIELD:settings.periodType:!=:byDate</numIndex>
                </AND>
              </displayCond>
              <config>
                <type>input</type>
                <size>1</size>
                <range>
                  <upper>999</upper>
                  <lower>-999</lower>
                </range>
                <eval>int,trim</eval>
              </config>
            </TCEforms>
          </settings.periodStart>
          <settings.periodDuration>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.duration</label>
              <displayCond>
                <AND>
                  <numIndex index="0">FIELD:settings.period:=:specific</numIndex>
                  <numIndex index="1">FIELD:settings.periodType:!=:byDate</numIndex>
                </AND>
              </displayCond>
              <config>
                <size>3</size>
                <range>
                  <upper>999</upper>
                  <lower>1</lower>
                </range>
                <default>1</default>
                <eval>int,trim</eval>
                <type>input</type>
              </config>
            </TCEforms>
          </settings.periodDuration>
          <settings.periodEndDate>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.end</label>
              <displayCond>FIELD:settings.periodType:=:byDate</displayCond>
              <config>
                <type>input</type>
                <size>8</size>
                <eval>date</eval>
                <checkbox>1</checkbox>
              </config>
            </TCEforms>
          </settings.periodEndDate>
          <!--- Quick Menu Type -->
          <settings.quickMenuType>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.quick_menu.type</label>
              <onChange>reload</onChange>
              <config>
                <type>select</type>
                <renderType>selectSingle</renderType>
                <items>
                  <numIndex index="0">
                    <numIndex index="0"></numIndex>
                    <numIndex index="1"></numIndex>
                  </numIndex>
                  <numIndex index="1">
                    <numIndex index="0">LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.quick_menu.type.genres</numIndex>
                    <numIndex index="1">genres</numIndex>
                  </numIndex>
                  <numIndex index="2">
                    <numIndex index="0">LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.quick_menu.type.venues</numIndex>
                    <numIndex index="1">venues</numIndex>
                  </numIndex>
                  <numIndex index="3">
                    <numIndex index="0">LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.quick_menu.type.eventTypes</numIndex>
                    <numIndex index="1">eventTypes</numIndex>
                  </numIndex>
                </items>
              </config>
            </TCEforms>
          </settings.quickMenuType>
          <settings.categoryConjunction>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.conjunction</label>
              <config>
                <type>select</type>
                <renderType>selectSingle</renderType>
                <items>
                  <numIndex index="0">
                    <numIndex index="0">LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.conjunction.or</numIndex>
                    <numIndex index="1">OR</numIndex>
                  </numIndex>
                  <numIndex index="1">
                    <numIndex index="0">LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.conjunction.and</numIndex>
                    <numIndex index="1">AND</numIndex>
                  </numIndex>
                </items>
              </config>
            </TCEforms>
          </settings.categoryConjunction>
          <!-- venues -->
          <settings.venues>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.venues</label>
              <config>
                <type>group</type>
                <internal_type>db</internal_type>
                <allowed>tx_t3events_domain_model_venue</allowed>
                <size>5</size>
                <autoSizeMax>20</autoSizeMax>
                <minitems>0</minitems>
                <maxitems>20</maxitems>
                <wizards>
                  <suggest>
                    <type>suggest</type>
                  </suggest>
                </wizards>
              </config>
            </TCEforms>
          </settings.venues>
          <!-- Genres -->
          <settings.genres>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.quick_menu.type.genres</label>
              <config>
                <type>group</type>
                <internal_type>db</internal_type>
                <allowed>tx_t3events_domain_model_genre</allowed>
                <size>5</size>
                <autoSizeMax>20</autoSizeMax>
                <minitems>0</minitems>
                <maxitems>40</maxitems>
                <wizards>
                  <suggest>
                    <type>suggest</type>
                  </suggest>
                </wizards>
              </config>
            </TCEforms>
          </settings.genres>
          <!-- Event types -->
          <settings.eventTypes>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.quick_menu.type.eventtypes</label>
              <config>
                <type>group</type>
                <internal_type>db</internal_type>
                <allowed>tx_t3events_domain_model_eventtype</allowed>
                <size>5</size>
                <autoSizeMax>20</autoSizeMax>
                <minitems>0</minitems>
                <maxitems>40</maxitems>
                <wizards>
                  <suggest>
                    <type>suggest</type>
                  </suggest>
                </wizards>
              </config>
            </TCEforms>
          </settings.eventTypes>
          <settings.categories>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.categories</label>
              <exclude>1</exclude>
              <config>
                <type>select</type>
                <renderType>selectTree</renderType>
                <autoSizeMax>50</autoSizeMax>
                <foreign_table>sys_category</foreign_table>
                <foreign_table_where> AND sys_category.sys_language_uid IN (-1, 0) ORDER BY sys_category.sorting ASC</foreign_table_where>
                <maxitems>9999</maxitems>
                <size>10</size>
                <treeConfig>
                  <appearance>
                    <expandAll>1</expandAll>
                    <showHeader>1</showHeader>
                  </appearance>
                  <parentField>parent</parentField>
                </treeConfig>
              </config>
            </TCEforms>
          </settings.categories>
        </el>
      </ROOT>
    </constraints>

    <!-- SHEET Template -->
    <template>
      <ROOT>
        <TCEforms>
          <sheetTitle>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.template</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
          <!-- Template Layout -->
          <settings.templateLayout>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.templateLayout</label>
              <config>
                <type>select</type>
                <renderType>selectSingle</renderType>
                <itemsProcFunc>DWenzel\T3events\Hooks\ItemsProcFunc->user_templateLayout</itemsProcFunc>
                <items>
                  <numIndex index="0" type="array">
                    <numIndex index="0"></numIndex>
                    <numIndex index="1"></numIndex>
                  </numIndex>
                </items>
              </config>
            </TCEforms>
          </settings.templateLayout>

          <settings.hideIfEmptyResult>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.hideIfEmptyResult</label>
              <config>
                <type>check</type>
              </config>
            </TCEforms>
          </settings.hideIfEmptyResult>

        </el>
      </ROOT>
    </template>

    <!-- SHEET PAGES -->
    <pages>
      <ROOT>
        <TCEforms>
          <sheetTitle>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.pages</sheetTitle>
        </TCEforms>
        <type>array</type>
        <el>
          <settings.listPid>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.listPage</label>
              <config>
                <type>group</type>
                <internal_type>db</internal_type>
                <allowed>pages</allowed>
                <minitems>0</minitems>
                <maxitems>1</maxitems>
                <wizards>
                  <suggest>
                    <type>suggest</type>
                  </suggest>
                </wizards>
              </config>
            </TCEforms>
          </settings.listPid>
          <settings.detailPid>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.detailPage</label>
              <config>
                <type>group</type>
                <internal_type>db</internal_type>
                <allowed>pages</allowed>
                <minitems>0</minitems>
                <maxitems>1</maxitems>
                <wizards>
                  <suggest>
                    <type>suggest</type>
                  </suggest>
                </wizards>
              </config>
            </TCEforms>
          </settings.detailPid>
          <settings.backPid>
            <TCEforms>
              <label>LLL:EXT:t3events/Resources/Private/Language/locallang_be.xlf:flexforms_general.backPage</label>
              <config>
                <type>group</type>
                <internal_type>db</internal_type>
                <allowed>pages</allowed>
                <minitems>0</minitems>
                <maxitems>1</maxitems>
                <wizards>
                  <suggest>
                    <type>suggest</type>
                  </suggest>
                </wizards>
              </config>
            </TCEforms>
          </settings.backPid>
        </el>
      </ROOT>
    </pages>
  </sheets>
</T3DataStructure>
